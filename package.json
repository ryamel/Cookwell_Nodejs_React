    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.6",
    "db": "^3.3.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "imagemin": "^8.0.0",
    "joi": "^17.4.2",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongo": "^0.1.0",
